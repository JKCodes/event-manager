<table>
  <tr>
    <th>Event Name</th>
    <th>Start Time</th>
    <th>End Time</th>
    <th>Participants</th>
  </tr>
  <% events.each do |event| %>
    <tr>
      <% if location %>
        <td><%= link_to event.name, location_event_path(location, event) %></td>
      <% else %>
        <td><%= link_to event.name, event_path(event) %></td>
      <% end %>
      <td><%= formatted_time(event.start_time) %></td>
      <td><%= formatted_time(event.end_time) %></td>
      <td><%= event.participants.count %>
    </tr>
  <% end %>
</table>